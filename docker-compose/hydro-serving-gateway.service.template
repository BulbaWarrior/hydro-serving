version: "3.5"

services:
  gateway:
    image: hydrorobot/serving-gateway:dd4495e5b3d1b79836de7f2f861ef50f95903e1a
    container_name: gateway
    ports:
      - "29090:9090"
      - "29091:9091"
    depends_on:
      - manager
    networks:
      - hydronet
    environment:
      - "API_GATEWAY_HOST=managerui"
      - "API_GATEWAY_GRPC_PORT=9090"
      - "API_GATEWAY_HTTP_PORT=8080"
      - "HTTP_PORT=9090"
      - "GRPC_PORT=9091"
