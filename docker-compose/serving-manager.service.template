version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:dab78c676ad4a82327a4ac9b24906583d20180bc
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
