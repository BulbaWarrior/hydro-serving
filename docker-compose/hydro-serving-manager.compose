version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:d27118a51151e2b6b0df748a2d6bcb3d20d19027
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    environment:
      - "DATABASE_HOST=postgres"
      - "NETWORK_NAME=demo_hydronet"
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
