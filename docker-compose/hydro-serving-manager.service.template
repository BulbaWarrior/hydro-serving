version: "3.5"

services:
  manager:
    image: bulbawarrior/serving-manager:9c1cba6ceeasdc844e8e3cd694f4724c29dsadasgs8f7c77e5
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
