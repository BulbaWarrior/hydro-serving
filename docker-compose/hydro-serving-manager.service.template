version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:07dc3c2284fa4f93f0a31d439a96236c3e618fa5
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    environment:
      CUSTOM_CONFIG: /app/config/dc.conf
    volumes:
      - ./dc.conf:/app/config/dc.conf:ro
      - /var/run/docker.sock:/var/run/docker.sock
