version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:64cea0cc1f81c99ab5e4df8659c0aee62aee08fd
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    environment:
      - "DATABASE_HOST=postgres"
      - "NETWORK_NAME=demo_hydronet"
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
