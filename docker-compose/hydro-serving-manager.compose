version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:378b281f6c31c0e638f72d9442b24b498f2602f0
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    environment:
      - "DATABASE_HOST=postgres"
      - "NETWORK_NAME=demo_hydronet"
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
