version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:868e5bdb8f98dda19c7409366c4e72b3c4873a57
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    environment:
      - "DATABASE_HOST=postgres"
      - "NETWORK_NAME=demo_hydronet"
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
