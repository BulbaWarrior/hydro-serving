version: "3.5"

services:
  manager:
    image: harbor.hydrosphere.io/hydro-test/serving-manager:83b6b199363d21d84a316acecd0b14aa9e3de3da
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    environment:
      - "DATABASE_HOST=postgres"
      - "NETWORK_NAME=demo_hydronet"
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
