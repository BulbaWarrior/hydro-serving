version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:2860de4422506d06e365ab20e2612cc315c0c995
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    environment:
      CUSTOM_CONFIG: /app/config/dc.conf
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
