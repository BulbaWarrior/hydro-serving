version: "3.5"

services:
  manager:
    image: hydrorobot/serving-manager:c6fc3acadf339ddbe778741af651a8ae513fe137
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
