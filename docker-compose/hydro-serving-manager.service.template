version: "3.5"

services:
  manager:
    image: hydrosphere/serving-manager:e8269a7772218b4de0631bb588dc7deba44b12ff
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    volumes:
      - /var/run/docker.sock:/var/run/docker.sock
