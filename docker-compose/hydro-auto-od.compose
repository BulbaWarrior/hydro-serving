version: "3.5"

services:
  auto-od:
    container_name: auto-od
    image: hydrosphere/auto-od:4dc0fc6ab58bdbd28160afb47dcb1c5759332a34
    environment:
      - MONGO_URL=mongodb
      - AWS_ACCESS_KEY_ID=minio
      - AWS_SECRET_ACCESS_KEY=minio123
      - S3_ENDPOINT=http://minio:9000
      - GRPC_PORT=5001
      - HS_CLUSTER_ADDRESS=http://managerui:8080
      - DEBUG=True
    depends_on:
      - mongodb
      - minio
    networks:
      - hydronet
      - extnet
