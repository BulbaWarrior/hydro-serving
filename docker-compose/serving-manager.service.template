version: "3.5"

services:
  manager:
    image: bulbawarrior/serving-manager:78417c00e0021c3ae002af1d66950c5977722ff3
    container_name: manager
    ports:
      - "19090:9090"
      - "19091:9091"
    depends_on:
      - postgres
    networks:
      - hydronet
      - extnet
    environment:
      CUSTOM_CONFIG: /app/config/dc.conf
    volumes:
      - ./dc.conf:/app/config/dc.conf:ro
      - /var/run/docker.sock:/var/run/docker.sock
